#include<stdio.h>
void main()
{
    char ch;
    scanf("%c",&ch);
    ch|=32;
    switch(ch)
    {
        case 'a':printf("vowel");break;
        case 'e':printf("vowel");break;
        case 'i':printf("vowel");break;
        case 'o':printf("vowel");break;
        case 'u':printf("vowel");break;
        default:printf("consonant");break;
    }
}
