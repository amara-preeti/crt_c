#include<stdio.h>
#include<math.h>
void main()
{
  int a,b,c;
  printf("enter a value");
  scanf("%d",&a);
  printf("enter b value");
  scanf("%d",&b);
  printf("enter c value");
  scanf("%d",&c);
  a=a+b+c;
  b=a-b-c;
  c=a-b-c;
  a=a-b-c;
  printf("After swapping a value is %d\n",a);
  printf("After swapping b value is %d\n",b);
   printf("After swapping c value is %d\n",c);
}

